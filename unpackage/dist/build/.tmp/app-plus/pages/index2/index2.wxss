
.pic_item {
	width: 440rpx;
	height: 130rpx;
	z-index: 110;
	position: absolute;
	right: 150rpx;
	-webkit-animation-delay: 0.25s;
	        animation-delay: 0.25s;
}
.pic_item :active {
	width: 440rpx;
	height: 130rpx;
	z-index: 110;
	position: absolute;
	opacity: 0.8;
	-webkit-transform: scale(1.05);
	    -ms-transform: scale(1.05);
	        transform: scale(1.05);
}
